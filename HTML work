         <script>
            const nav = document.getElementById('trigram');
            console.log(nav);
            nav.addEventListener('click',() => {
               const currentAttrValue = nav.getAttribute('aria-expanded');
               const newAttrValue = currentAttrValue === 'false' ? 'true' : 'false';
               nav.setAttribute('aria-expanded',newAttrValue);
            });
         </script>
